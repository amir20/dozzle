    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.1.0",
    "c8": "^10.1.3",
    "concurrently": "^9.1.2",
    "eventsourcemock": "^2.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "simple-git-hooks": "^2.12.1",
    "ts-node": "^10.9.2",
