{
 "name": "dozzle",
 "version": "3.0.3",
 "description": "Realtime log viewer for docker containers. ",
 "scripts": {
  "prestart": "yarn clean",
  "watch": "npm-run-all -p watch:*",
  "watch:assets": "webpack --mode=development --watch",
  "watch:server": "reflex -c .reflex",
  "dev": "npm-run-all -p dev-server watch:server",
  "dev-server": "webpack serve --mode=development",
  "prebuild": "yarn clean",
  "build": "yarn webpack --mode=production",
  "clean": "rm -rf static/ a_main-packr.go",
  "release": "release-it",
  "test": "TZ=UTC jest",
  "integration": "docker-compose -f integration/docker-compose.test.yml up --build --force-recreate --exit-code-from integration"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/amir20/dozzle.git"
 },
 "author": "",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/amir20/dozzle/issues"
 },
 "homepage": "https://github.com/amir20/dozzle#readme",
 "dependencies": {
  "@babel/polyfill": "^7.12.1",
  "ansi-to-html": "^0.6.14",
  "buefy": "^0.9.4",
  "bulma": "^0.9.1",
  "date-fns": "^2.16.1",
  "dompurify": "^2.2.2",
  "hotkeys-js": "^3.8.1",
  "lodash.debounce": "^4.0.8",
  "lodash.throttle": "^4.1.1",
  "semver": "^7.3.2",
  "splitpanes": "^2.3.4",
  "store": "^2.0.12",
  "vue": "^2.6.12",
  "vue-meta": "^2.4.0",
  "vue-router": "^3.4.9",
  "vuex": "^3.5.1"
 },
 "devDependencies": {
  "@babel/core": "^7.12.3",
  "@babel/plugin-transform-runtime": "^7.12.1",
  "@babel/preset-env": "^7.12.1",
  "@vue/component-compiler-utils": "^3.2.0",
  "@vue/test-utils": "^1.1.1",
  "autoprefixer": "^10.0.2",
  "babel-jest": "^26.6.3",
  "babel-loader": "^8.2.1",
  "caniuse-lite": "^1.0.30001157",
  "css-loader": "^5.0.1",
  "eventsourcemock": "^2.0.0",
  "html-webpack-plugin": "^4.5.0",
  "husky": "^4.3.0",
  "jest": "^26.6.3",
  "jest-serializer-vue": "^2.0.2",
  "lint-staged": "^10.5.1",
  "mini-css-extract-plugin": "^1.3.0",
  "npm-run-all": "^4.1.5",
  "postcss": "^8.1.9",
  "postcss-loader": "^4.1.0",
  "prettier": "^2.1.2",
  "release-it": "^14.2.1",
  "sass": "^1.29.0",
  "sass-loader": "^10.1.0",
  "vue-hot-reload-api": "^2.3.4",
  "vue-jest": "^3.0.7",
  "vue-loader": "^15.9.5",
  "vue-style-loader": "^4.1.2",
  "vue-template-compiler": "^2.6.12",
  "webpack": "^5.4.0",
  "webpack-cli": "^4.2.0",
  "webpack-dev-server": "^3.11.0",
  "webpack-pwa-manifest": "^4.2.0"
 },
 "husky": {
  "hooks": {
   "pre-commit": "lint-staged"
  }
 },
 "lint-staged": {
  "*.{js,vue,css}": [
   "prettier --write"
  ]
 },
 "release-it": {
  "github": {
   "release": true
  },
  "npm": {
   "publish": false
  }
 }
}
